{
    "name": "@democrance/utils",
    "version": "0.14.3",
    "description": "Set of popular utils",
    "sideEffects": false,
    "keywords": [
        "console",
        "utils",
        "log"
    ],
    "repository": "daniil4udo/utils",
    "license": "MIT",
    "author": "Daniil Chumachenko <daniil4udo@gmail.com>",
    "type": "module",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./*": {
            "require": "./dist/*.cjs",
            "import": "./dist/*.js",
            "types": "./dist/*.d.ts"
        },
        "./preset": {
            "require": "./dist/preset/autoImportUtilsPreset.cjs",
            "import": "./dist/preset/autoImportUtilsPreset.js",
            "types": "./dist/preset/autoImportUtilsPreset.d.ts"
        }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "./scripts/build.sh",
        "prepublishOnly": "pnpm build",
        "release": "bumpp --commit --tag --no-push && pnpm publish --access public",
        "lint": "eslint --fix --ext .vue,.ts,.js .",
        "typedoc": "typedoc --plugin typedoc-plugin-markdown ./lib/index.ts",
        "test": "vitest --ui --coverage"
    },
    "dependencies": {
        "defu": "^6.1.2",
        "fast-equals": "^5.0.1",
        "file-saver-es": "^2.0.5",
        "is-number": "^7.0.0",
        "micro-memoize": "^4.1.2",
        "micromatch": "^4.0.5",
        "perfect-debounce": "^1.0.0",
        "proper-url-join": "^2.1.1",
        "rfdc": "^1.3.0",
        "text-clipper": "^2.2.0",
        "zipcelx": "^1.6.2"
    },
    "devDependencies": {
        "@democrance/eslint-config": "^0.8.0",
        "@types/file-saver-es": "^2.0.1",
        "@types/micromatch": "^4.0.2",
        "@types/node": "^18.16.19",
        "@types/proper-url-join": "^2.1.1",
        "@types/url-template": "^2.0.28",
        "@types/zipcelx": "^1.5.0",
        "@vitest/coverage-c8": "^0.33.0",
        "@vitest/coverage-v8": "^0.33.0",
        "@vitest/ui": "^0.33.0",
        "@vue/tsconfig": "^0.4.0",
        "bumpp": "^9.1.1",
        "concurrently": "^8.2.0",
        "eslint": "^8.44.0",
        "jsdom": "^22.1.0",
        "tsup": "^7.1.0",
        "typedoc": "^0.24.8",
        "typedoc-plugin-markdown": "^3.15.3",
        "typescript": "^5.1.6",
        "url-template": "^3.1.0",
        "vitest": "^0.33.0"
    },
    "packageManager": "pnpm@8.6.0",
    "engines": {
        "node": ">=16"
    },
    "pnpm": {
        "overrides": {
            "file-saver": "npm:file-saver-es@^2.0.5",
            "semver": "^7.5.2",
            "uuid": "^9.0.0"
        }
    }
}
